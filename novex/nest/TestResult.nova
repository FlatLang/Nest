package "novex/nest"

import "nova/time/Timer"

class TestResult {
    visible Bool success
    visible Timer timer
    visible String functionName
    visible String description
    
    public construct(visible Bool success, visible Timer timer, visible String functionName, visible String description = null) {}
    
    getHeader() => description ?: functionName
    
    public toString() => "\"#getHeader()\" => " + (success ? "Success" : "Failure")
}